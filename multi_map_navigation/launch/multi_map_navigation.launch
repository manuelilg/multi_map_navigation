<launch>
  <!--<include file="$(find multi_map_navigation)/launch/map_store.launch" />-->
  <node name="multi_level_map_server" pkg="multi_level_map_server" type="multi_level_map_server">
    <param name="map_file" value="$(find cartographer_turtlebot)/maps/multi_level.yaml" />
  </node>

  <!-- Start the level mux -->
  <node name="level_mux" pkg="multi_level_map_utils" type="level_mux">
    <param name="default_current_level" value="level1"/>
    <remap from="/level_mux/map" to="/map"/>
  </node>

  <!--<node pkg="multi_map_navigation" type="multi_map_navigation" name="multi_map_navigation" >-->
    <!--&lt;!&ndash;<param name="definition_file" value="$(find multi_map_navigation)/config/multi_level_db/cfg.yaml" />-->
    <!--<param name="transition_types" value="elevator_blast" />- -->
    <!--<param name="robot_namespace" value="$(arg modelName)"/>-->
    <!--<param name="base_frame" value="base_link"/>&ndash;&gt;-->
  <!--</node>-->

  <node pkg="multi_map_navigation" type="multi_map_navigation_new" name="multi_map_navigation" output="screen">
    <param name="definition_file" value="$(find multi_map_navigation)/config/cfg.yaml" />
    <param name="transition_types" value="elevator_blast" />

    <param name="robot_namespace" value=""/>
    <param name="base_frame" value="base_link"/>
  </node>

  <include file="$(find turtlebot_navigation)/launch/includes/move_base.launch.xml">
<!--    <arg name="custom_param_file" value="$(find turtlebot_navigation)/param/velodyne_costmap_params_with_ns.yaml"/>-->
    <arg name="laser_topic" value="scan"/>
  </include>

  <include file="$(find turtlebot_navigation)/launch/includes/amcl/amcl.launch.xml">
    <arg name="use_map_topic" value="true"/>
    <arg name="initial_pose_x"  default="3.0"/>
    <arg name="initial_pose_y"  default="0.0"/>
    <arg name="initial_pose_a"  default="1.5"/>
  </include>

</launch>

